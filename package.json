{
  "name": "dl-starter-new",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --filter=web",
    "build": "turbo run build",
    "start": "turbo run start --filter=web",
    "lint": "turbo run lint",
    "format": "prettier -w .",
    "test": "turbo run test",
    "test:unit": "turbo run test:unit",
    "test:e2e": "playwright test",
    "typecheck": "turbo run typecheck",
    "debug": "tsx scripts/debug.ts",
    "debug:sentry": "tsx scripts/debug.ts sentry:check",
    "debug:env": "tsx scripts/debug.ts env:check",
    "debug:health": "tsx scripts/debug.ts dev:health",
    "new-app": "tsx scripts/new-app.ts",
    "new-repo": "tsx scripts/new-repo-from-template.ts", 
    "doctor": "tsx scripts/starter-doctor.ts",
    "constitution:update": "tsx scripts/update-constitution-checksum.ts"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "commander": "^14.0.0",
    "prettier": "^3.6.2",
    "turbo": "^2.3.3",
    "tsx": "^4.20.5",
    "typescript": "^5"
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67"
}