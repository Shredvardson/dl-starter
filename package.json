{
  "name": "dl-starter-new",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --filter=web",
    "build": "turbo run build",
    "start": "turbo run start --filter=web",
    "lint": "turbo run lint",
    "format": "prettier -w .",
    "pretest": "tsx scripts/pretest-env-check.ts",
    "test": "npm run pretest && turbo run test",
    "test:unit": "npm run pretest && turbo run test:unit", 
    "test:e2e": "npm run pretest && turbo run test:e2e --filter=web",
    "test:e2e:ci": "tsx scripts/pretest-env-check.ts && playwright test --config playwright.config.ts",
    "typecheck": "turbo run typecheck",
    "debug": "tsx scripts/debug.ts",
    "debug:sentry": "tsx scripts/debug.ts sentry:check",
    "debug:env": "tsx scripts/debug.ts env:check",
    "debug:health": "tsx scripts/debug.ts dev:health",
    "new-app": "tsx scripts/new-app.ts",
    "new-repo": "tsx scripts/new-repo-from-template.ts",
    "doctor": "tsx scripts/starter-doctor.ts",
    "doctor:report": "tsx scripts/starter-doctor.ts --report artifacts/doctor-report.md",
    "preflight": "tsx scripts/preflight.ts",
    "constitution:update": "tsx scripts/update-constitution-checksum.ts",
    "check:react-versions": "node scripts/check-react-versions.mjs",
    "preflight:placeholders": "bash -lc 'PATTERN=\"{{\"; PATTERN+=\"PLACEHOLDER\"; PATTERN+=\"}}\"; ! grep -R \"$PATTERN\" . -n --exclude-dir=node_modules --exclude-dir=.turbo'",
    "git:start": "bash scripts/git-workflow.sh start",
    "git:status": "bash scripts/git-workflow.sh status",
    "git:cleanup": "bash scripts/git-workflow.sh cleanup-dry",
    "git:cleanup-force": "bash scripts/git-workflow.sh cleanup",
    "git:finish": "bash scripts/git-workflow.sh finish-branch",
    "docs:check": "tsx scripts/docs-check.ts",
    "pr:create": "tsx scripts/create-pr.ts",
    "wiki:generate": "node scripts/generate-wiki.js",
    "commands:generate": "node scripts/generate-command-index.js",
    "commands:check": "node scripts/generate-command-index.js && git diff --exit-code docs/commands/index.json || (echo 'Command index out of sync. Run: npm run commands:generate' && exit 1)",
    "new-spec": "node scripts/new-spec.mjs",
    "telemetry:report": "tsx scripts/telemetry-report.ts",
    "telemetry:biweekly": "node scripts/telemetry/generate-biweekly-report.mjs",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "clean": "rm -rf .turbo coverage test-results playwright-report 2>/dev/null || true; find . -type f -name '*.tsbuildinfo' -print -delete 2>/dev/null || true"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@eslint/eslintrc": "^3.1.0",
    "@playwright/test": "^1.55.0",
    "commander": "^14.0.1",
    "eslint": "^9.35.0",
    "fast-glob": "^3.3.3",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "turbo": "^2.3.3",
    "typescript": "^5"
  },
  "overrides": {
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "zod": "3.23.8"
  },
  "packageManager": "pnpm@9.12.0"
}
